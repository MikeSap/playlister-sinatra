<form action="/songs" method="POST">
 <h3> <label>Song Name:</label></h3><br>
  <input type="text" id="name" name="song[name]">

  <h3>Artist</h3>
   <input type="text" id="new_artist" name="artist[name]">
  <br>
  <h3>Genres</h3>
    <% @genres.each do |genre| %>
    <input type="checkbox" id="genre" name="[genres][]"  value="<%= genre.id %>"><%= genre.name %><br>
  <% end %><br>
   <input type='submit' value='submit'>
</form>
